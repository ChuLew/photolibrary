<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.TilePane?>

<AnchorPane fx:id="rootPane" prefHeight="541.0" prefWidth="828.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="photo65.view.Gallery">
   <children>
      <ImageView fx:id="invisIV" fitHeight="300.0" fitWidth="600.0" layoutX="120.0" layoutY="20.0" onMouseClicked="#imagePress" pickOnBounds="true" preserveRatio="true" />
      <ScrollPane fx:id="scrollPane" layoutX="17.0" layoutY="14.0" prefHeight="320.0" prefWidth="795.0">
         <content>
            <TilePane fx:id="tilePane" onMouseClicked="#imagePress" prefHeight="160.0" prefWidth="790.0" />
         </content>
      </ScrollPane>
      <Button fx:id="addBtn" layoutX="14.0" layoutY="359.0" mnemonicParsing="false" onAction="#add" prefHeight="106.0" prefWidth="147.0" text="Add Photo" />
      <Button fx:id="remBtn" layoutX="181.0" layoutY="359.0" mnemonicParsing="false" onAction="#remove" prefHeight="106.0" prefWidth="147.0" text="Remove Photo" />
      <Button fx:id="disBtn" layoutX="338.0" layoutY="359.0" mnemonicParsing="false" prefHeight="106.0" prefWidth="147.0" text="Display Photo" />
      <Button fx:id="copyBtn" layoutX="506.0" layoutY="359.0" mnemonicParsing="false" onAction="#copy" prefHeight="106.0" prefWidth="147.0" text="Copy Photo" />
      <Button fx:id="moveBtn" layoutX="662.0" layoutY="359.0" mnemonicParsing="false" onAction="#move" prefHeight="106.0" prefWidth="147.0" text="Move Photo" />
      <Button fx:id="logBtn" layoutX="241.0" layoutY="479.0" mnemonicParsing="false" onAction="#onLogout" prefHeight="39.0" prefWidth="147.0" text="Logout" />
      <Button fx:id="retBtn" layoutX="405.0" layoutY="479.0" mnemonicParsing="false" onAction="#onReturn" prefHeight="39.0" prefWidth="147.0" text="Return" />
   </children>
</AnchorPane>
